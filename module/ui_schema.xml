<?xml version="1.0"?>
<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:jr="http://openrosa.org/javarosa" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Fill This In</h:title>
    <model>
      <instance>
        <faims id="Fill_This_In">
          <style>
            <orientation>
              <orientation/>
            </orientation>
            <even>
              <layout_weight/>
            </even>
            <large>
              <layout_weight/>
            </large>
          </style>
          <Test>
            <Test>
              <connectToBluetooth/>
              <testText/>
              <testPrint/>
            </Test>
          </Test>
          <logic/>
        </faims>
      </instance>
    </model>
  </h:head>
  <h:body>
    <group ref="style">
      <label/>
      <group ref="orientation">
        <label/>
        <input ref="orientation">
          <label>horizontal</label>
        </input>
      </group>
      <group ref="even">
        <label/>
        <input ref="layout_weight">
          <label>1</label>
        </input>
      </group>
      <group ref="large">
        <label/>
        <input ref="layout_weight">
          <label>3</label>
        </input>
      </group>
    </group>
    <group ref="Test">
      <label>{Test}</label>
      <group ref="Test">
        <label>{Test}</label>
        <trigger ref="connectToBluetooth">
          <label>{connectToBluetooth}</label>
        </trigger>
        <input ref="testText">
          <label>{testText}</label>
          <!--WARNING: No type t was given for this view; assuming it is an input-->
        </input>
        <trigger ref="testPrint">
          <label>{testPrint}</label>
        </trigger>
      </group>
    </group>
  </h:body>
</h:html>
